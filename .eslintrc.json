{
    "env": {
        "es2024": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "sourceType": "module",
        "requireConfigFile": false,
        "babelOptions": {
          "plugins": [
            "@babel/plugin-syntax-import-attributes"
          ]
        }
    }
}
